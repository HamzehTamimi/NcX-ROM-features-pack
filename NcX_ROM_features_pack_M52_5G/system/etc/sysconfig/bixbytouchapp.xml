<?xml version="1.0" encoding="UTF-8"?>
<config>
    <!-- These are the standard packages that are white-listed to always have internet access while in power save mode, even if they aren't in the foreground. -->
    <!-- Bixby touch client must always have network access for provide service,and must always start service in background to show trigger animation. -->

    <allow-in-power-save package="com.samsung.android.bixbytouch" />
    <allow-in-power-save-except-idle package="com.samsung.android.bixbytouch" />
</config>